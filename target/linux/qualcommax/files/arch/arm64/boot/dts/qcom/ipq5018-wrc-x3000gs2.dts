/dts-v1/;

#include "ipq5018.dtsi"
#include "ipq5018-ess.dtsi"
#include "ipq5018-nss.dtsi"
#include "ipq5018-common.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "ELECOM WRC-X3000GS2";
	compatible = "elecom,wrc-x3000gs2", "qcom,ipq5018";

	aliases {
		serial0 = &blsp1_uart1;
		led-boot = &led_power_green;
		led-failsafe = &led_power_red;
		led-running = &led_power_green;
		led-upgrade = &led_power_green;
		label-mac-device = <&dp1>;
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs-append = " root=/dev/ubiblock0_1";
	};

	keys {
		compatible = "gpio-keys";

		switch-router {
			label = "router";
			gpios = <&tlmm 14 GPIO_ACTIVE_HIGH>;
			linux,code = <BTN_0>;
			linux,input-type = <EV_SW>;
		};

		reset-button {
			label = "reset";
			gpios = <&tlmm 22 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps-button {
			label = "wps";
			gpios = <&tlmm 38 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led-0 {
			gpios = <&tlmm 12 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_RED>;
			function = LED_FUNCTION_WPS;
		};

		led_power_green: power-green {
			gpios = <&tlmm 13 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_POWER;
		};

		led_power_red: power-red {
			gpios = <&tlmm 16 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_RED>;
			function = LED_FUNCTION_POWER;
		};

		led-3 {
			gpios = <&tlmm 24 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WLAN_5GHZ;
			linux,default-trigger = "phy1radio";
		};

		led-4 {
			gpios = <&tlmm 25 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_RED>;
			function = LED_FUNCTION_WLAN_5GHZ;
		};

		led-5 {
			gpios = <&tlmm 26 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WLAN_2GHZ;
			linux,default-trigger = "phy0radio";
		};

		led-6 {
			gpios = <&tlmm 28 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_RED>;
			function = LED_FUNCTION_WLAN_2GHZ;
		};

		led-7 {
			gpios = <&tlmm 46 GPIO_ACTIVE_HIGH>;
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WAN;
		};
	};
};

&qpic_nand {
	status = "okay";

	pinctrl-0 = <&qpic_pins>;
	pinctrl-names = "default";

	nand@0 {
		compatible = "spi-nand";
		reg = <0>;

		nand-ecc-engine = <&qpic_nand>;
		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

		partitions {
			compatible = "qcom,smem-part";
			#address-cells = <1>;
			#size-cells = <1>;

			partition-0-appsblenv {
				compatible = "fixed-partitions";
				#address-cells = <1>;
				#size-cells = <1>;

				label = "0:appsblenv";

				partition@0 {
					label = "env-data";
					reg = <0x00000000 0x00040000>;

					nvmem-layout {
						compatible = "u-boot,env";
						#address-cells = <1>;
						#size-cells = <1>;

						macaddr_appsblenv_ethaddr: ethaddr {
							compatible = "mac-base";
							#nvmem-cell-cells = <1>;
						};
					};
				};
			};
		};
	};
};

&dp1 {
	status = "okay";

	label = "wan";
	nvmem-cells = <&macaddr_appsblenv_ethaddr 2>;
	nvmem-cell-names = "mac-address";
};

&dp2 {
	status = "okay";

	nvmem-cells = <&macaddr_appsblenv_ethaddr 0>;
	nvmem-cell-names = "mac-address";

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&mdio1 {
	status = "okay";

	pinctrl-0 = <&mdio1_pins>, <&switch_reset_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 39 GPIO_ACTIVE_LOW>;

	qca8337_0: ethernet-phy@0 {
		reg = <0>;
	};

	qca8337_1: ethernet-phy@1 {
		reg = <1>;
	};

	qca8337_2: ethernet-phy@2 {
		reg = <2>;
	};

	qca8337_3: ethernet-phy@3 {
		reg = <3>;
	};

	qca8337: ethernet-switch@18 {
		compatible = "qca,qca8337";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <18>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@1 {
				reg = <1>;
				label = "lan1";
				phy-handle = <&qca8337_0>;
			};

			port@2 {
				reg = <2>;
				label = "lan2";
				phy-handle = <&qca8337_1>;
			};

			port@3 {
				reg = <3>;
				label = "lan3";
				phy-handle = <&qca8337_2>;
			};

			port@4 {
				reg = <4>;
				label = "lan4";
				phy-handle = <&qca8337_3>;
			};

			port@6 {
				reg = <6>;
				phy-mode = "sgmii";
				ethernet = <&dp2>;
				qca,sgmii-enable-pll;

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};
		};
	};
};

&tlmm {
	switch_reset_pins: switch-reset-pins {
		pins = "gpio39";
		function = "gpio";
		drive-strength = <8>;
		bias-pull-down;
	};
};

&q6v5_wcss {
	boot-args = <0x02 0x04 0x02 0x12 0x00 0x00>;
};

&wifi {
	status = "okay";

	qcom,rproc = <&q6_wcss_pd1>;
	qcom,userpd-subsys-name = "q6v5_wcss_userpd1";
	qcom,ath11k-calibration-variant = "ELECOM-WRC-X3000GS2";
};

&wifi1 {
	status = "okay";

	qcom,rproc = <&q6_wcss_pd2>;
	qcom,userpd-subsys-name = "q6v5_wcss_userpd2";
	qcom,ath11k-calibration-variant = "ELECOM-WRC-X3000GS2";
};
