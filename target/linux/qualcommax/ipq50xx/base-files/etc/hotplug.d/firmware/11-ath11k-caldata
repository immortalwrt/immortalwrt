#!/bin/sh

[ -e /lib/firmware/$FIRMWARE ] && exit 0

. /lib/functions/caldata.sh

board=$(board_name)

case "$FIRMWARE" in
"ath11k/IPQ5018/hw1.0/cal-ahb-c000000.wifi.bin")
	case "$board" in
	elecom,wrc-x3000gs2|\
	iodata,wn-dax3000gr|\
	glinet,gl-b3000|\
	xiaomi,ax6000|\
		caldata_extract "0:art" 0x1000 0x20000
		;;
	linksys,mr5500|\
	linksys,mx2000|\
	linksys,mx5500|\
	linksys,spnmx56|\
	yuncore,ax830|\
	yuncore,ax850)
		caldata_extract "0:ART" 0x1000 0x20000
		ath11k_remove_regdomain
		;;
	jdcloud,re-cs-03)
		caldata_extract_mmc "0:ART" 0x1000 0x20000
		;;
	esac
	;;
"ath11k/QCN6122/hw1.0/cal-ahb-b00a040.wifi.bin")
	case "$board" in
	elecom,wrc-x3000gs2|\
	iodata,wn-dax3000gr|\
	glinet,gl-b3000)
		caldata_extract "0:art" 0x26800 0x20000
		;;
	linksys,mx2000)
		caldata_extract "0:ART" 0x26800 0x20000
		ath11k_remove_regdomain
		;;
	yuncore,ax830)
		caldata_extract "0:ART" 0x4c000 0x20000
		ath11k_remove_regdomain
		;;
	jdcloud,re-cs-03)
		caldata_extract_mmc "0:ART" 0x26800 0x20000
		;;
	esac
	;;
"ath11k/QCN9074/hw1.0/cal-pci-0000:01:00.0.bin")
	case "$board" in
	linksys,mr5500|\
	linksys,mx5500|\
	linksys,spnmx56|\
	yuncore,ax850)
		caldata_extract "0:ART" 0x26800 0x20000
		ath11k_remove_regdomain
		;;
	xiaomi,ax6000)
		caldata_extract "0:art" 0x26800 0x20000
		;;
	esac
	;;
*)
	exit 1
	;;
esac
