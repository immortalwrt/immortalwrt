+static void __exit imq_unhook(void)
+{
+	int hook_index = 0;
+	for(hook_index = 0; hook_index < hook_list_length; hook_index++)
+	{
+		nf_unregister_hook( (hook_list+hook_index) );
+	}
+	nf_unregister_queue_imq_handler();
+
+	kfree(hook_list);
+	hook_list_length = 0;
