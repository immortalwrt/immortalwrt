#include "ipq8174-mx4x00.dtsi"

&tlmm {
	iot_pins: iot-pins {
		recovery-pins {
			pins = "gpio22";
			function = "gpio";
			drive-strength = <8>;
			input;
		};

		reset-pins {
			pins = "gpio21";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};
};

&blsp1_uart3 {
	status = "okay";

	pinctrl-0 = <&hsuart_pins>, <&iot_pins>;
	pinctrl-names = "default";

	/* Silicon Labs EFR32MG21 IoT */
};

&qpic_nand {
	status = "okay";

	partitions {
		status = "disabled";
	};

	nand@0 {
		reg = <0>;

		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "0:sbl1";
				reg = <0x00000000 0x00100000>;
				read-only;
			};

			partition@100000 {
				label = "0:mibib";
				reg = <0x00100000 0x00100000>;
				read-only;
			};

			partition@200000 {
				label = "0:bootconfig";
				reg = <0x00200000 0x00080000>;
				read-only;
			};

			partition@280000 {
				label = "0:bootconfig1";
				reg = <0x00280000 0x00080000>;
				read-only;
			};

			partition@300000 {
				label = "0:qsee";
				reg = <0x00300000 0x00300000>;
				read-only;
			};

			partition@600000 {
				label = "0:qsee_1";
				reg = <0x00600000 0x00300000>;
				read-only;
			};

			partition@900000 {
				label = "0:devcfg";
				reg = <0x00900000 0x00080000>;
				read-only;
			};

			partition@980000 {
				label = "0:devcfg_1";
				reg = <0x00980000 0x00080000>;
				read-only;
			};

			partition@a00000 {
				label = "0:apdp";
				reg = <0x00a00000 0x00080000>;
				read-only;
			};

			partition@a80000 {
				label = "0:apdp_1";
				reg = <0x00a80000 0x00080000>;
				read-only;
			};

			partition@b00000 {
				label = "0:rpm";
				reg = <0x00b00000 0x00080000>;
				read-only;
			};

			partition@b80000 {
				label = "0:rpm_1";
				reg = <0x00b80000 0x00080000>;
				read-only;
			};

			partition@c00000 {
				label = "0:cdt";
				reg = <0x00c00000 0x00080000>;
				read-only;
			};

			partition@c80000 {
				label = "0:cdt_1";
				reg = <0x00c80000 0x00080000>;
				read-only;
			};

			partition@d00000 {
				label = "0:appsblenv";
				reg = <0x00d00000 0x00080000>;
			};

			partition@d80000 {
				label = "0:appsbl";
				reg = <0x00d80000 0x00100000>;
				read-only;
			};

			partition@e80000 {
				label = "0:appsbl_1";
				reg = <0x00e80000 0x00100000>;
				read-only;
			};

			partition@f80000 {
				label = "0:art";
				reg = <0x00f80000 0x00080000>;
				read-only;
			};

			partition@1000000 {
				label = "u_env";
				reg = <0x01000000 0x00040000>;
			};

			partition@1040000 {
				label = "s_env";
				reg = <0x01040000 0x00020000>;
			};

			partition@1060000 {
				label = "devinfo";
				reg = <0x01060000 0x00020000>;
				read-only;

				nvmem-layout {
					compatible = "ascii-eq-delim-env";

					hw_mac_addr: hw_mac_addr {
						compatible = "mac-base";
						#nvmem-cell-cells = <1>;
					};
				};
			};

			partition@1080000 {
				label = "kernel";
				reg = <0x01080000 0x09600000>;
			};

			partition@1680000 {
				label = "rootfs";
				reg = <0x01680000 0x09000000>;
			};

			partition@a680000 {
				label = "alt_kernel";
				reg = <0x0a680000 0x09600000>;
			};

			partition@ac80000 {
				label = "alt_rootfs";
				reg = <0x0ac80000 0x09000000>;
			};

			partition@13c80000 {
				label = "sysdiag";
				reg = <0x13c80000 0x00200000>;
				read-only;
			};

			partition@13e80000 {
				label = "0:ethphyfw";
				reg = <0x13e80000 0x00080000>;
				read-only;
			};

			partition@13f00000 {
				label = "syscfg";
				reg = <0x13f00000 0x0b800000>;
				read-only;
			};

			partition@1f700000 {
				label = "0:wififw";
				reg = <0x1f700000 0x00900000>;
				read-only;
			};
		};
	};
};

&dp2 {
	status = "okay";
	phy-handle = <&qca8075_1>;
	label = "wan";
};

&dp3 {
	status = "okay";
	phy-handle = <&qca8075_2>;
	label = "lan1";
};

&dp4 {
	status = "okay";
	phy-handle = <&qca8075_3>;
	label = "lan2";
};

&dp5 {
	status = "okay";
	phy-handle = <&qca8075_4>;
	label = "lan3";
};
