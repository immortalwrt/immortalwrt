/dts-v1/;

#include "ipq5018.dtsi"
#include "ipq5018-ess.dtsi"
#include "ipq5018-nss.dtsi"
#include "ipq5018-common.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "JDCloud RE-CS-03";
	compatible = "jdcloud,re-cs-03", "qcom,ipq5018";

	aliases {
		sdhc1 = &sdhc_1;
		serial0 = &blsp1_uart1;
		serial1 = &blsp1_uart2;

		ethernet0 = &dp1;
		ethernet1 = &dp2;
		label-mac-device = &dp2;

		led-boot = &led_status_blue;
		led-failsafe = &led_status_red;
		led-running = &led_status_green;
		led-upgrade = &led_status_red;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	leds {
		compatible = "gpio-leds";

		led_status_blue: status-blue {
			label = "blue:status";
			gpios = <&tlmm 31 GPIO_ACTIVE_HIGH>;
		};

		led_status_green: status-green {
			label = "green:status";
			gpios = <&tlmm 32 GPIO_ACTIVE_HIGH>;
		};

		led_status_red: status-red {
			label = "red:status";
			gpios = <&tlmm 33 GPIO_ACTIVE_HIGH>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&tlmm 25 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			debounce-interval = <60>;
		};

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&tlmm 38 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			debounce-interval = <60>;
		};
	};
};

&sdhc_1 {
	status = "okay";

	pinctrl-0 = <&emmc_pins>;
	pinctrl-names = "default";

	max-frequency = <192000000>;

	/delete-property/ mmc-hs400-1_8v;
	mmc-hs200-1_8v;
	mmc-ddr-1_8v;
};

&dp1 {
	status = "okay";
};

&dp2 {
	status = "okay";

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&mdio1 {
	status = "okay";

	pinctrl-0 = <&mdio1_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 39 GPIO_ACTIVE_LOW>;

	qca8337_0: ethernet-phy@0 {
		reg = <0>;
	};

	qca8337_1: ethernet-phy@1 {
		reg = <1>;
	};

	qca8337_2: ethernet-phy@2 {
		reg = <2>;
	};

	qca8337_3: ethernet-phy@3 {
		reg = <3>;
	};

	qca8337: ethernet-switch@17 {
		compatible = "qca,qca8337";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <17>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				label = "cpu";
				phy-mode = "sgmii";
				ethernet = <&dp2>;
				qca,sgmii-enable-pll;

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};

			port@1 {
				reg = <1>;
				label = "lan3";
				phy-handle = <&qca8337_0>;
			};

			port@2 {
				reg = <2>;
				label = "lan2";
				phy-handle = <&qca8337_1>;
			};

			port@3 {
				reg = <3>;
				label = "lan1";
				phy-handle = <&qca8337_2>;
			};

			port@4 {
				reg = <4>;
				label = "wan";
				phy-handle = <&qca8337_3>;
			};
		};
	};
};

&q6v5_wcss {
	boot-args = <0x02 0x04 0x02 0x0f 0x00 0x00>;
};

&wifi0 {
	status = "okey";
	qcom,ath11k-calibration-variant = "JDC-RE-CS-03";
};

&wifi1 {
	status = "okey";
	qcom,ath11k-calibration-variant = "JDC-RE-CS-03";
};
