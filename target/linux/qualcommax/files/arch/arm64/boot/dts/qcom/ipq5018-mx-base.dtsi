#include "ipq5018.dtsi"
#include "ipq5018-ess.dtsi"
#include "ipq5018-nss.dtsi"
#include "ipq5018-common.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {

	aliases {
		led-boot = &led_system_blue;
		led-failsafe = &led_system_red;
		led-running = &led_system_blue;
		led-upgrade = &led_system_red;
		serial0 = &blsp1_uart1;
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs-append = " root=/dev/ubiblock0_0";
	};

	keys {
		compatible = "gpio-keys";

		wps-button {
			label = "wps";
			gpios = <&tlmm 27 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		reset-button {
			label = "reset";
			gpios = <&tlmm 28 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "pwm-leds";

		led_system_red: system-red {
			color = <LED_COLOR_ID_RED>;
			function = LED_FUNCTION_POWER;
			pwms = <&pwm 3 1250000>;
			max-brightness = <255>;
		};

		green {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_POWER;
			pwms = <&pwm 0 1250000>;
			max-brightness = <255>;
		};

		led_system_blue: system-blue {
			color = <LED_COLOR_ID_BLUE>;
			function = LED_FUNCTION_POWER;
			pwms = <&pwm 1 1250000>;
			max-brightness = <255>;
		};
	};
};

&pwm {
	status = "okay";

	#pwm-cells = <2>;
	pinctrl-0 = <&pwm_pins>;
	pinctrl-names = "default";
};

&qpic_nand {
	status = "okay";

	pinctrl-0 = <&qpic_pins>;
	pinctrl-names = "default";

	partitions {
		status = "disabled";
	};

	nand@0 {
		compatible = "spi-nand";
		reg = <0>;

		nand-ecc-engine = <&qpic_nand>;
		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "0:SBL1";
				reg = <0x00000000 0x00080000>;
				read-only;
			};

			partition@80000 {
				label = "0:MIBIB";
				reg = <0x00080000 0x00080000>;
				read-only;
			};

			partition@100000 {
				label = "0:QSEE";
				reg = <0x00100000 0x00100000>;
				read-only;
			};

			partition@200000 {
				label = "0:DEVCFG";
				reg = <0x00200000 0x00040000>;
				read-only;
			};

			partition@240000 {
				label = "0:CDT";
				reg = <0x00240000 0x00040000>;
				read-only;
			};

			partition@280000 {
				label = "0:APPSBLENV";
				reg = <0x00280000 0x00080000>;
			};

			partition@300000 {
				label = "0:APPSBL";
				reg = <0x00300000 0x00140000>;
				read-only;
			};

			partition@440000 {
				label = "0:ART";
				reg = <0x00440000 0x00100000>;
				read-only;
			};

			partition@540000 {
				label = "0:TRAINING";
				reg = <0x00540000 0x00080000>;
				read-only;
			};

			partition@5c0000 {
				label = "u_env";
				reg = <0x005c0000 0x00080000>;
			};

			partition@640000 {
				label = "s_env";
				reg = <0x00640000 0x00040000>;
			};

			partition@680000 {
				label = "devinfo";
				reg = <0x00680000 0x00040000>;
				read-only;

				nvmem-layout {
					compatible = "ascii-eq-delim-env";
					#address-cells = <1>;
					#size-cells = <1>;

					hw_mac_addr: hw_mac_addr {
						compatible = "mac-base";
						#nvmem-cell-cells = <1>;
					};
				};
			};

			partition@6c0000 {
				label = "kernel";
				reg = <0x006c0000 0x05200000>;
			};

			partition@ec0000 {
				label = "rootfs";
				reg = <0x00ec0000 0x04a00000>;
			};

			partition@58c0000 {
				label = "alt_kernel";
				reg = <0x058c0000 0x05200000>;
			};

			partition@60c0000 {
				label = "alt_rootfs";
				reg = <0x060c0000 0x04a00000>;
			};

			partition@aac0000 {
				label = "sysdiag";
				reg = <0x0aac0000 0x00200000>;
				read-only;
			};

			partition@acc0000 {
				label = "syscfg";
				reg = <0x0acc0000 0x04400000>;
				read-only;
			};
		};
	};
};

&tlmm {
	pwm_pins: pwm-pins {
		mux_1 {
			pins = "gpio1";
			function = "pwm1";
			drive-strength = <8>;
		};

		mux_2 {
			pins = "gpio30";
			function = "pwm3";
			drive-strength = <8>;
		};

		mux_3 {
			pins = "gpio46";
			function = "pwm0";
			drive-strength = <8>;
		};
	};
};

&dp1 {
	status = "okay";
};

&dp2 {
	status = "okay";

	nvmem-cells = <&hw_mac_addr 0>;
	nvmem-cell-names = "mac-address";

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&mdio1 {
	status = "okay";

	pinctrl-0 = <&mdio1_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 39 GPIO_ACTIVE_LOW>;

	qca8337_1: ethernet-phy@1 {
		reg = <1>;
	};

	qca8337_2: ethernet-phy@2 {
		reg = <2>;
	};

	qca8337_3: ethernet-phy@3 {
		reg = <3>;
	};

	qca8337_4: ethernet-phy@4 {
		reg = <4>;
	};

	qca8337: ethernet-switch@17 {
		compatible = "qca,qca8337";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <17>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@2 {
				reg = <2>;
				label = "wan";
				phy-handle = <&qca8337_1>;
			};

			port@3 {
				reg = <3>;
				label = "lan3";
				phy-handle = <&qca8337_2>;
			};

			port@4 {
				reg = <4>;
				label = "lan2";
				phy-handle = <&qca8337_3>;
			};

			port@5 {
				reg = <5>;
				label = "lan1";
				phy-handle = <&qca8337_4>;
			};

			port@6 {
				reg = <6>;
				label = "cpu";
				phy-mode = "sgmii";
				ethernet = <&dp2>;
				qca,sgmii-enable-pll;

				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};
		};
	};
};
