
config system
	option hostname 'MyWRT'
	option timezone '<+08>-8'
	option ttylogin '0'
	option log_size '128'
	option urandom_seed '0'
	option description 'Qmodem Division'
	option notes 'Designed For Basic User(NonUart)
'
	option zonename 'Asia/Kuala Lumpur'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '5'
	option zram_comp_algo 'lzo'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config led 'led_wan'
	option name 'WAN'
	option sysfs 'green:internet'
	option trigger 'netdev'
	option dev 'wwan0'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led 'led_wan_port_link'
	option name 'WAN-PORT-LINK'
	option sysfs '90000.mdio-1:1c:green:wan'
	option trigger 'netdev'
	option mode 'tx rx link_10 link_100 link_1000 link_2500'
	option dev 'wan'

config imm_init
	option ipv6 '2'

config led
	option name 'Phone'
	option sysfs 'green:phone'
	option trigger 'netdev'
	option dev 'wan'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led
	option name 'Wi-Fi'
	option sysfs 'green:wifi'
	option trigger 'netdev'
	option dev 'phy0-ap0'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led 'nipq__led9'
	option name '4-1_net_indicator'
	option sysfs 'ipq::led9'
	option trigger 'netdev'
	option dev 'wwan0_1'
	option mode 'tx rx'


