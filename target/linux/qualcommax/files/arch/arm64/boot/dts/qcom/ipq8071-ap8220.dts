/dts-v1/;

#include "ipq8071-edgecore.dtsi"

/ {
	model = "Aliyun AP8220";
	compatible = "aliyun,ap8220", "qcom,ipq8074";

	leds {
		compatible = "gpio-leds";

		led_pw: pw {
			label = "pw:status";
			gpios = <&tlmm 46 GPIO_ACTIVE_HIGH>;
		};

		led_bt: bt {
			label = "bt:status";
			gpios = <&tlmm 50 GPIO_ACTIVE_HIGH>;
		};
	};

	gpio-export {
		compatible = "gpio-export";

		ble-power {
			gpio-export,name = "ble_power";
			gpio-export,output = <1>;
			gpios = <&tlmm 54 GPIO_ACTIVE_HIGH>;
		};
	};
};

&blsp1_spi1 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		spi-max-frequency = <50000000>;
		reg = <0>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "0:sbl1";
				reg = <0x00000000 0x00050000>;
			};

			partition@50000 {
				label = "0:mibib";
				reg = <0x00050000 0x00010000>;
			};

			partition@60000 {
				label = "0:qsee";
				reg = <0x00060000 0x00180000>;
			};

			partition@1e0000 {
				label = "0:devcfg";
				reg = <0x001e0000 0x00010000>;
			};

			partition@1f0000 {
				label = "0:apdp";
				reg = <0x001f0000 0x00010000>;
			};

			partition@200000 {
				label = "0:rpm";
				reg = <0x00200000 0x00040000>;
			};

			partition@240000 {
				label = "0:cdt";
				reg = <0x00240000 0x00010000>;
			};

			partition@250000 {
				label = "0:appsblenv";
				reg = <0x00250000 0x00010000>;
			};

			partition@260000 {
				label = "0:appsbl";
				reg = <0x00260000 0x000a0000>;
			};

			partition@300000 {
				label = "0:art";
				reg = <0x00300000 0x00040000>;
			};

			partition@340000 {
				label = "0:ethphyfw";
				reg = <0x00340000 0x00080000>;
			};

			partition@3c0000 {
				label = "product_info";
				reg = <0x003c0000 0x00010000>;
			};

			partition@3d0000 {
				label = "mtdoops";
				reg = <0x003d0000 0x00020000>;
			};

			partition@3f0000 {
				label = "priv_data1";
				reg = <0x003f0000 0x00010000>;
			};
		};
	};
};

&qpic_nand {
	status = "okay";

	partitions {
		status = "disabled";
	};

	nand@0 {
		reg = <0>;

		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "rootfs";
				reg = <0x0 0x0>;
			};
		};
	};
};

&dp5 {
	status = "okay";
	phy-handle = <&qca8081_24>;
	label = "wan";
};

&dp6 {
	status = "okay";
	phy-handle = <&qca8081_28>;
	label = "lan";
};

&wifi {
	status = "okay";
	qcom,ath11k-fw-memory-mode = <1>;
	qcom,ath11k-calibration-variant = "Aliyun-AP8220";
};
