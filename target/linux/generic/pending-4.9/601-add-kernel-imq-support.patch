+/* returns number freed */
+int free_null_terminated_string_array(char** strs)
+{
+	unsigned long str_index = 0;
+	if(strs != NULL)
+	{
+		for(str_index=0; strs[str_index] != NULL; str_index++)
+		{
+			kfree(strs[str_index]);
+		}
+		kfree(strs);
+	}
+	return str_index;
+}
